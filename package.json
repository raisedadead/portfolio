{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "is-ci || husky",
    "postinstall": "turbo cf-typegen",
    "develop": "astro dev",
    "build": "astro build",
    "preview": "wrangler dev",
    "deploy": "wrangler deploy",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix --cache",
    "format": "prettier --write . --cache",
    "format:check": "prettier --check . --cache",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest --update",
    "test:watch:ui": "vitest --ui",
    "test:watch": "vitest",
    "check": "astro check",
    "cf-typegen": "wrangler types",
    "lighthouse": "lhci collect --config=.github/lighthouse/lhci.json",
    "lighthouse:assert": "lhci assert --config=.github/lighthouse/lhci.json",
    "turbo:build": "dotenv -e .env -- turbo build",
    "turbo:test": "dotenv -e .env -- turbo test",
    "turbo:test:coverage": "dotenv -e .env -- turbo test:coverage",
    "turbo:lint": "dotenv -e .env -- turbo lint",
    "turbo:lint:fix": "dotenv -e .env -- turbo lint:fix",
    "turbo:format": "dotenv -e .env -- turbo format",
    "turbo:format:check": "dotenv -e .env -- turbo format:check",
    "turbo:check": "dotenv -e .env -- turbo check",
    "turbo:develop": "dotenv -e .env -- turbo develop",
    "turbo:preview": "dotenv -e .env -- turbo preview",
    "turbo:deploy": "dotenv -e .env -- turbo deploy",
    "turbo:lighthouse": "dotenv -e .env -- turbo lighthouse"
  },
  "lint-staged": {
    "**/*.{mjs,cjs,js,jsx,ts,tsx,astro}": [
      "eslint --fix --cache",
      "prettier --write --cache"
    ],
    "**/*.{json,md}": [
      "prettier --write --cache"
    ]
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/cloudflare": "12.6.10",
    "@astrojs/react": "4.4.0",
    "@astrojs/sitemap": "3.6.0",
    "@calcom/embed-react": "1.5.3",
    "@headlessui/react": "2.2.9",
    "@heroicons/react": "2.2.0",
    "@tailwindcss/vite": "4.1.14",
    "astro": "5.14.6",
    "astro-loader-hashnode": "1.2.0",
    "clsx": "2.1.1",
    "framer-motion": "12.23.24",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sharp": "0.34.4",
    "shiki": "3.13.0",
    "tailwind-merge": "3.3.1",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20251011.0",
    "@eslint/js": "9.38.0",
    "@lhci/cli": "0.15.1",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/typography": "0.5.19",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "24.8.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "8.46.1",
    "@typescript-eslint/parser": "8.46.1",
    "@vitejs/plugin-react": "5.0.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "dotenv-cli": "10.0.0",
    "eslint": "9.38.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.0",
    "globals": "16.4.0",
    "happy-dom": "20.0.7",
    "husky": "9.1.7",
    "is-ci": "4.1.0",
    "jest-axe": "10.0.0",
    "jsdom": "27.0.1",
    "lighthouse": "13.0.0",
    "lint-staged": "16.2.4",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.7.0",
    "tailwindcss": "4.1.14",
    "turbo": "2.5.8",
    "typescript": "5.9.3",
    "vite": "7.1.10",
    "vitest": "3.2.4",
    "wrangler": "4.43.0"
  },
  "packageManager": "pnpm@10.18.3",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
