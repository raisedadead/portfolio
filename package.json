{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "next build",
    "build:for-edge": "npx @cloudflare/next-on-pages --experimental-minify",
    "start": "next dev",
    "develop": "next dev",
    "test": "jest .",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix ."
    ]
  },
  "dependencies": {
    "@builder.io/partytown": "0.7.5",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "3.20.6"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "0.3.0",
    "@cloudflare/workers-types": "4.20230115.0",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "autoprefixer": "10.4.13",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-react": "7.32.2",
    "husky": "8.0.3",
    "jest": "29.4.2",
    "jest-environment-jsdom": "29.4.2",
    "lint-staged": "13.1.2",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.2",
    "tailwindcss": "3.2.7",
    "typescript": "4.9.5",
    "vercel": "^28.15.3",
    "wrangler": "2.9.1"
  },
  "pnpm": {
    "patchedDependencies": {
      "@cloudflare/next-on-pages@0.3.0": "patches/@cloudflare__next-on-pages@0.3.0.patch"
    }
  }
}
