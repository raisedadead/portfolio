{
  "private": true,
  "name": "raisedadead",
  "description": "Nothing fancy here, just one of my personal websites.",
  "license": "ISC",
  "author": "mrugesh mohapatra <me@raisedadead.com> (https://raisedadead.com)",
  "homepage": "/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raisedadead/raisedadead.git"
  },
  "bugs": {
    "url": "https://github.com/raisedadead/raisedadead/issues"
  },
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "commit": "git-cz",
    "commit:retry": "git-cz --retry",
    "eject": "react-scripts eject",
    "lint": "lint-staged",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "commitizen.config.js"
    }
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "uikit": "^3.0.0-beta.34"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.0",
    "commitizen": "^3.0.5",
    "cz-customizable": "^5.2.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "1.15.3",
    "prettier-package-json": "^2.0.0",
    "react-scripts": "^2.1.1",
    "react-snap": "^1.22.1"
  },
  "engines": {
    "node": ">8"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  }
}
