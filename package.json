{
  "name": "@raisedadead/portfolio",
  "description": "mrugesh mohapatra's personal portfolio",
  "license": "ISC",
  "scripts": {
    "clean": "gatsby clean",
    "dev": "npm run develop",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve --port 8000",
    "format": "npm run lint -- --fix",
    "lint": "eslint .",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:record": "cypress run --record",
    "test": "npm run test:unit",
    "test:e2e": "npm run test:e2e:dev:run",
    "test:e2e:ci": "npm run test:e2e:ci:run",
    "test:e2e:dev:run": "start-server-and-test develop http://localhost:8000 cy:run",
    "test:e2e:dev:watch": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci:run": "start-server-and-test serve http://localhost:8000 cy:run",
    "test:e2e:ci:rec": "start-server-and-test serve http://localhost:8000 cy:run:record",
    "test:unit": "jest .",
    "test:unit:update": "jest . --update-snapshot",
    "test:unit:watch": "jest . --watch-all"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{js,json,css,md}": [
      "prettier --write"
    ]
  },
  "version": "1.0.0",
  "main": "\"N/A\"",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raisedadead/portfolio.git"
  },
  "author": "mrugesh mohapatra <ama+npm@raisedadead.com>",
  "bugs": {
    "url": "https://github.com/raisedadead/portfolio/issues"
  },
  "homepage": "https://github.com/raisedadead/portfolio#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.1",
    "babel-jest": "^25.3.0",
    "babel-preset-gatsby": "^0.3.1",
    "cross-env": "^7.0.2",
    "cypress": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "gatsby": "^2.20.12",
    "gatsby-plugin-google-analytics": "^2.2.2",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.2.2",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sitemap": "^2.3.1",
    "gatsby-source-filesystem": "^2.2.2",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.1",
    "prettier": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "start-server-and-test": "^1.10.11"
  }
}
