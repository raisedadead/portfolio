{
  "name": "gatsby-starter",
  "description": "This starter ships with the main Gatsby files and a small collection of plugins to start a PWA with Google Analytics, Tag Manager, Manifest, SEO tools, Stripe for product listings and checkout, plus the basic TDD/automated-test tools preferred by the Gatsby core contributors: Jest & Cypress https://gatsby-starter.davesabine.com.",
  "license": "CC-BY-4.0",
  "scripts": {
    "build": "gatsby build",
    "cy:open": "cypress open --config baseUrl=http://localhost:8000",
    "cy:run": "cypress run --key process.env.CYPRESS_UPLOAD_KEY",
    "develop": "gatsby develop",
    "format": "npm run lint -- --fix",
    "lint": "eslint .",
    "serve": "gatsby serve",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "dependencies": {
    "@emotion/core": "^10.0.9",
    "@emotion/is-prop-valid": "^0.7.3",
    "@emotion/styled": "^10.0.9",
    "emotion-theming": "^10.0.9",
    "gatsby": "^2.1.32",
    "gatsby-plugin-create-client-paths": "^2.0.5",
    "gatsby-plugin-google-analytics": "^2.0.17",
    "gatsby-plugin-google-tagmanager": "^2.0.10",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-netlify-functions": "^0.0.3",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.9",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-sitemap": "^2.0.9",
    "gatsby-plugin-typography": "^2.2.10",
    "gray-percentage": "^2.0.0",
    "mineral-ui": "^0.57.0",
    "netlify-identity-widget": "^1.5.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.18",
    "typography": "^0.16.18"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-pro": "^5.8.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.5.0",
    "babel-preset-gatsby": "^0.1.9",
    "cross-env": "^5.2.0",
    "cypress": "^3.1.2",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.3.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jest-dom": "^3.1.3",
    "prettier": "1.16.4",
    "react-test-renderer": "^16.8.4",
    "start-server-and-test": "^1.7.10"
  }
}
