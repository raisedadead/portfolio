{
  "private": true,
  "name": "raisedadead",
  "description": "Nothing fancy here, just one of my personal websites.",
  "license": "ISC",
  "author": "mrugesh mohapatra <me@raisedadead.com> (https://raisedadead.com)",
  "homepage": "/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raisedadead/raisedadead.git"
  },
  "bugs": {
    "url": "https://github.com/raisedadead/raisedadead/issues"
  },
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "postbuild": "SKIP_PREFLIGHT_CHECK=true react-snap",
    "build-storybook": "build-storybook -s public",
    "commit": "git-cz",
    "commit:retry": "git-cz --retry",
    "develop": "SKIP_PREFLIGHT_CHECK=true npm run start:develop",
    "eject": "react-scripts eject",
    "lint": "lint-staged",
    "start": "SKIP_PREFLIGHT_CHECK=true npm run start:app",
    "start:app": "SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "start:develop": "run-p start:storybook start:app",
    "start:storybook": "start-storybook -p 9009 -s public",
    "test": "SKIP_PREFLIGHT_CHECK=true react-scripts test --env=jsdom"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "commitizen.config.js"
    }
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-ga": "^2.5.6",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "react-spring": "^7.2.8",
    "uikit": "^3.0.0-rc.26"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.0",
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addons": "^5.0.0",
    "@storybook/react": "^4.1.4",
    "babel-loader": "^8.0.4",
    "commitizen": "^3.0.7",
    "cz-customizable": "^5.2.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "hygen-cra": "^1.0.0",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.15.3",
    "prettier-package-json": "^2.0.0",
    "react-scripts": "^2.1.5",
    "react-snap": "^1.23.0",
    "react-test-renderer": "^16.7.0"
  },
  "engines": {
    "node": ">8"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  }
}
