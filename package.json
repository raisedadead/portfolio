{
  "name": "@raisedadead/portfolio",
  "description": "mrugesh mohapatra's personal portfolio",
  "license": "ISC",
  "scripts": {
    "clean": "gatsby clean",
    "dev": "npm run develop",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "gatsby develop",
    "serve": "gatsby serve --port 8000",
    "format": "npm run lint -- --fix",
    "lint": "eslint .",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:record": "cypress run --record",
    "test": "npm run test:unit",
    "test:e2e": "npm run test:e2e:dev:run",
    "test:e2e:ci": "npm run test:e2e:ci:run",
    "test:e2e:dev:run": "start-server-and-test develop http://localhost:8000 cy:run",
    "test:e2e:dev:watch": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci:run": "start-server-and-test serve http://localhost:8000 cy:run",
    "test:e2e:ci:rec": "start-server-and-test serve http://localhost:8000 cy:run:record",
    "test:unit": "jest .",
    "test:unit:update": "jest . --update-snapshot",
    "test:unit:watch": "jest . --watch-all"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{js,json,css,md}": [
      "prettier --write"
    ]
  },
  "version": "1.0.0",
  "main": "\"N/A\"",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raisedadead/portfolio.git"
  },
  "author": "mrugesh mohapatra <ama+npm@raisedadead.com>",
  "bugs": {
    "url": "https://github.com/raisedadead/portfolio/issues"
  },
  "homepage": "https://github.com/raisedadead/portfolio#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^0.9.0",
    "cross-env": "^7.0.3",
    "cypress": "^5.6.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "gatsby": "^2.28.2",
    "gatsby-background-image": "^1.3.1",
    "gatsby-plugin-emotion": "^4.5.0",
    "gatsby-plugin-google-analytics": "^2.8.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.9.0",
    "gatsby-plugin-offline": "^3.7.0",
    "gatsby-plugin-react-helmet": "^3.7.0",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sharp": "^2.10.1",
    "gatsby-plugin-sitemap": "^2.9.0",
    "gatsby-source-filesystem": "^2.8.0",
    "gatsby-transformer-sharp": "^2.9.0",
    "husky": "^4.3.6",
    "identity-obj-proxy": "^3.0.0",
    "intersection-observer": "^0.12.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-loading-skeleton": "^2.1.1",
    "start-server-and-test": "^1.11.6",
    "tailwindcss": "^1.9.6",
    "twin.macro": "^1.12.1"
  },
  "babelMacros": {
    "twin": {
      "config": "tailwind.config.js",
      "preset": "emotion",
      "debugProp": true,
      "debugPlugins": false,
      "debug": false
    }
  }
}
