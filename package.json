{
  "name": "@raisedadead/portfolio",
  "description": "mrugesh mohapatra's personal portfolio",
  "license": "ISC",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve --port 8000",
    "format": "npm run lint -- --fix",
    "lint": "eslint .",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:record": "cypress run --record",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci:dev": "start-server-and-test serve http://localhost:8000 cy:run",
    "test:e2e:ci": "start-server-and-test serve http://localhost:8000 cy:run:record",
    "test": "jest --coverage",
    "test:update": "jest -u",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.9",
    "@emotion/is-prop-valid": "^0.7.3",
    "@emotion/styled": "^10.0.9",
    "@fortawesome/fontawesome-pro": "^5.8.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "emotion-theming": "^10.0.9",
    "gatsby": "^2.2.8",
    "gatsby-image": "^2.0.34",
    "gatsby-plugin-create-client-paths": "^2.0.5",
    "gatsby-plugin-google-analytics": "^2.0.17",
    "gatsby-plugin-google-tagmanager": "^2.0.10",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-netlify-functions": "^0.0.3",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.10",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-sharp": "^2.0.30",
    "gatsby-plugin-sitemap": "^2.0.10",
    "gatsby-plugin-typography": "^2.2.10",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-sharp": "^2.1.17",
    "gray-percentage": "^2.0.0",
    "mineral-ui": "^0.57.0",
    "netlify-identity-widget": "^1.5.2",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.18",
    "typography": "^0.16.18"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.5.0",
    "babel-preset-gatsby": "^0.1.9",
    "cross-env": "^5.2.0",
    "cypress": "^3.2.0",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jest-dom": "^3.1.3",
    "lint-staged": "^8.1.5",
    "prettier": "1.16.4",
    "react-test-renderer": "^16.8.5",
    "start-server-and-test": "^1.7.12"
  },
  "version": "1.0.0",
  "main": "\"N/A\"",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raisedadead/portfolio.git"
  },
  "author": "mrugesh mohapatra <ama+npm@raisedadead.com>",
  "bugs": {
