{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:for-edge": "npx @cloudflare/next-on-pages",
    "build": "next build",
    "dev": "pnpm turbo:develop",
    "develop": "next dev",
    "develop:edge": "pnpm build:for-edge && wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "postbuild": "next-sitemap --config='sitemap.config.mjs'",
    "preinstall": "npx only-allow pnpm",
    "prepare": "is-ci || husky",
    "start": "turbo dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "turbo:develop": "turbo develop",
    "turbo:build:for-edge": "turbo build:for-edge",
    "turbo:build": "turbo build",
    "turbo:test": "turbo test",
    "turbo:test:watch": "turbo test:watch",
    "turbo:lint": "turbo lint",
    "turbo:format": "turbo format"
  },
  "lint-staged": {
    "**/*.{mjs,cjs,js,jsx,ts,tsx,yaml,yml}": [
      "pnpm format"
    ]
  },
  "dependencies": {
    "@headlessui/react": "2.0.4",
    "@heroicons/react": "2.1.4",
    "@next/third-parties": "14.2.4",
    "framer-motion": "11.2.10",
    "graphql": "16.8.2",
    "graphql-request": "7.0.1",
    "next": "14.2.4",
    "next-sitemap": "4.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "swr": "2.2.5",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.11.3",
    "@cloudflare/workers-types": "4.20240614.0",
    "@eslint/compat": "1.1.0",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.5.0",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.13",
    "@testing-library/react": "16.0.0",
    "@types/node": "20.14.5",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "@vitejs/plugin-react": "4.3.1",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-next-on-pages": "1.11.3",
    "eslint-plugin-react": "7.34.3",
    "globals": "15.6.0",
    "husky": "9.0.11",
    "is-ci": "3.0.1",
    "jsdom": "24.1.0",
    "lint-staged": "15.2.7",
    "next-router-mock": "0.9.13",
    "postcss": "8.4.38",
    "prettier": "3.3.2",
    "prettier-plugin-tailwindcss": "0.6.5",
    "tailwindcss": "3.4.4",
    "turbo": "2.0.4",
    "typescript": "5.4.5",
    "vercel": "34.2.7",
