{
  "name": "portfolio",
  "dockerFile": "Dockerfile",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",

  "forwardPorts": [4321],

  "portsAttributes": {
    "4321": {
      "label": "Astro",
      "onAutoForward": "openBrowser"
    }
  },
  "otherPortsAttributes": {
    "onAutoForward": "ignore"
  },

  "onCreateCommand": "pnpm install",
  "postCreateCommand": "pnpm run dev"
}
