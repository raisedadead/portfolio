{
  "name": "mrugesh-portfolio",
  "image": "mcr.microsoft.com/devcontainers/typescript-node:24",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "forwardPorts": [4321],
  "portsAttributes": {
    "4321": {
      "label": "Website",
      "onAutoForward": "notify"
    }
  },
  "containerEnv": {
    "ASTRO_TELEMETRY_DISABLED": "1"
  },
  "updateContentCommand": "git pull --rebase origin main && pnpm install",
  "postAttachCommand": {
    "Develop": "bash -c 'pnpm develop'"
  }
}
